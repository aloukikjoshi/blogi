{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py",
            "methods": ["GET", "POST"]
        }
    ],
    "functions": [
        {
            "src": "api/*.py",
            "use": "@vercel/python"
        }
    ]
}